	module.exports = "<div id=\"dialog\" class=\"dialog\" style=\"display: block;\">\n    <div class=\"dialog-window\">\n        <div class=\"dialog-content\">\n            <div id=\"dialog-placeholder\" class=\"dialog-placeholder\">\n                <p class=\"txt-emph mb\" v-html=\"message\"></p>\n                <div v-if=\"isError\" class=\"box-center m-mt\">\n                    <div class=\"base-btn\"><a :href=\"rootUrl\">再読み込み</a></div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<div class=\"overlay overlay-black\"></div>";

