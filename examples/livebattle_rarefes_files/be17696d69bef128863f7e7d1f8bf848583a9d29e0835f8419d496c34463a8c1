/* 77 */
/***/ function(module, exports, __webpack_require__) {

	__webpack_require__(78);

	module.exports = {
		props: ['battle', 'battles', 'thumbs', 'linkType'],
		template: __webpack_require__(80),
		filters: {
			timeLeft: function (timestamp, currentTimestamp) {
				var delta = new Date(timestamp) - new Date(currentTimestamp);
				if (delta < 0)return "00:00:00";
				var hours = parseInt(delta / 1000 / 60 / 60, 10);
				var minutes = parseInt((delta / 1000 / 60) - hours * 60, 10);
				var seconds = parseInt(delta / 1000 % 60, 10);
				return (hours < 10 ? "0" + hours : hours)
					+ ":"
					+ (minutes < 10 ? "0" + minutes : minutes)
					+ ":"
					+ (seconds < 10 ? "0" + seconds : seconds)
			}
		},
		methods: {
			getUserProfileUrl: function (id) {
				return this.$root.baseUrl + '/mypage/user_profile/id/' + id;
			},
			getJoinUrl: function (battle) {
				var type = "detail";
				if (battle.isFinish) {
					type = "result";
				} else if (battle.isLounge) {
					type = "join_lounge_live_battle";
				} else if (!battle.isOwn) {
					type = "join_invited_live_battle";
				}
				return this.$root.baseUrl + '/livebattle/' + type + '/' + battle.liveBattleId;
			},
			numberFormat: function (num) {
				return String(num).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1,');
			},
		}
	};


