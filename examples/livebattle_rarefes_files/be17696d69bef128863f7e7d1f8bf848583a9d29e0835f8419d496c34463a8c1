/***/ function(module, exports, __webpack_require__) {

	//var Vue = require('vue');
	var LiveBattleView = __webpack_require__(73);
	// var DeckPositionChange = require('../views/deckPositionChange');
	var MessageDialog = __webpack_require__(47);

	Vue.use(__webpack_require__(49));

	//外部のコンストラクタがない場合を考慮
	var LiveBattleApp = App || Vue.extend();

	new LiveBattleApp({
		el: '#livebattle',
		data: {
			battles: {},
			thumbs: {},
			isLoading: false,
			currentView: 'LiveBattleView',
			errors: {
				0: 'ネットワーク接続に失敗しました<br>ページを再読み込みしてください',
				400: 'データの取得に失敗しました<br>ページを再読み込みしてください',
				401: '一定時間アクセスがありませんでした<br>ページの再読み込みを行ってください'
			},
			message: '',
			isError: false,
			isMessage: false
		},
		created: function () {
			this.load()
		},
		computed: {
			//apiのurlを取得
			apis: function () {
				return {
					getLiveBattles: this.baseUrl + '/api/livebattle'
				}
			}
		}, methods: {
			currentBattleUrl: function () {
				return this.baseUrl + '/livebattle';
			},
			soundPlay: function (se) {
				if (imas.Webview !== undefined) {
					imas.Webview.sound.se(se);
				}
			},
			load: function (params) {
				params = (params == null) ? {} : params;
				var url = this.apis.getLiveBattles;
				this.isLoading = true;

				this.$http.post(url, params, {emulateJSON: true, timeout: 5000}).then(function (response) {
					this.render(response.data);
					this.isLoading = false;
				}, function (response) {
					this.$emit('errorResponse', response.status);
				});
			},
			reload:function () {
				this.$root.soundPlay('SE_DECIDE1');
				this.load();
			},
			render: function (response) {
				this.$set('battles', response);

				var idList = [];
				for (var i in response['users']) {
					idList.push(response['users'][i].snsId);
				}
				if (typeof GREE.api === 'function') {
					var that = this;
					GREE.api('/thumbnail', {
						"user_ids": idList
					}, function (response) {
						var thumbs = {};
						for (var key in response.data) {
							if (response.data.hasOwnProperty(key)) {
								var image = response.data[key];
								thumbs[key] = image.large;
							}
						}
						that.$set('thumbs', thumbs);
					});
				}
			}
		},
		//エントリーに共通処理を書くと別の場所であるコンポーネントだけを使う時に困るかも
		events: {
			post: function (url, callback, params, isCsrf) {
				params = (params == null) ? {} : params;

				this.isLoading = true;

				this.$http.post(url, params, {emulateJSON: true, timeout: 5000}).then(function (response) {
					if (typeof callback === 'function') {
						callback(response.data);
					}
					this.isLoading = false;
				}, function (response) {
					this.$emit('errorResponse', response.status);
				});
			},
			successResponse: function (msg) {
				console.log(msg);
			},
			errorResponse: function (status) {
				this.message = this.errors[status];

				this.isLoading = false;
				this.isError = true;

				document.body.classList.add('dialog-open');
			}
		},
		components: {
			LiveBattleView: LiveBattleView,
			MessageDialog: MessageDialog
		}
	});

