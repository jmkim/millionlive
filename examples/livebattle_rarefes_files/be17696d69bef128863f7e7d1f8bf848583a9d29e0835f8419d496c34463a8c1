/***/ },
/* 68 */
/***/ function(module, exports, __webpack_require__) {

	/**
	 * Mime Interceptor.
	 */

	var _ = __webpack_require__(50);

	module.exports = {

	    request: function (request) {

	        if (request.emulateJSON && _.isPlainObject(request.data)) {
	            request.headers['Content-Type'] = 'application/x-www-form-urlencoded';
	            request.data = _.url.params(request.data);
	        }

	        if (_.isObject(request.data) && /FormData/i.test(request.data.toString())) {
	            delete request.headers['Content-Type'];
	        }

	        if (_.isPlainObject(request.data)) {
	            request.data = JSON.stringify(request.data);
	        }

	        return request;
	    },

	    response: function (response) {

	        try {
	            response.data = JSON.parse(response.data);
	        } catch (e) {}

	        return response;
	    }

	};


/***/ },
/* 69 */
/***/ function(module, exports, __webpack_require__) {

	/**
	 * Header Interceptor.
	 */

	var _ = __webpack_require__(50);

	module.exports = {

	    request: function (request) {

	        request.method = request.method.toUpperCase();
	        request.headers = _.extend({}, _.http.headers.common,
	            !request.crossOrigin ? _.http.headers.custom : {},
	            _.http.headers[request.method.toLowerCase()],
	            request.headers
	        );

	        if (_.isPlainObject(request.data) && /^(GET|JSONP)$/i.test(request.method)) {
	            _.extend(request.params, request.data);
	            delete request.data;
	        }

	        return request;
	    }

	};


/***/ },
/* 70 */
/***/ function(module, exports, __webpack_require__) {

	/**
	 * CORS Interceptor.
	 */

	var _ = __webpack_require__(50);
	var xdrClient = __webpack_require__(71);
	var xhrCors = 'withCredentials' in new XMLHttpRequest();
	var originUrl = _.url.parse(location.href);

	module.exports = {

	    request: function (request) {

	        if (request.crossOrigin === null) {
	            request.crossOrigin = crossOrigin(request);
	        }

	        if (request.crossOrigin) {

	            if (!xhrCors) {
	                request.client = xdrClient;
	            }

	            request.emulateHTTP = false;
	        }

	        return request;
	    }

	};

	function crossOrigin(request) {

	    var requestUrl = _.url.parse(_.url(request));

	    return (requestUrl.protocol !== originUrl.protocol || requestUrl.host !== originUrl.host);
	}


/***/ },
/* 71 */
/***/ function(module, exports, __webpack_require__) {

	/**
	 * XDomain client (Internet Explorer).
	 */

	var _ = __webpack_require__(50);
	var Promise = __webpack_require__(59);

	module.exports = function (request) {
	    return new Promise(function (resolve) {

	        var xdr = new XDomainRequest(), response = {request: request}, handler;

	        request.cancel = function () {
	            xdr.abort();
	        };

	        xdr.open(request.method, _.url(request), true);

	        handler = function (event) {

	            response.data = xdr.responseText;
	            response.status = xdr.status;
	            response.statusText = xdr.statusText;

	            resolve(response);
	        };

	        xdr.timeout = 0;
	        xdr.onload = handler;
	        xdr.onabort = handler;
	        xdr.onerror = handler;
	        xdr.ontimeout = function () {};
	        xdr.onprogress = function () {};

	        xdr.send(request.data);
	    });
	};


/***/ },
/* 72 */
/***/ function(module, exports, __webpack_require__) {

	/**
	 * Service for interacting with RESTful services.
	 */

	var _ = __webpack_require__(50);

	function Resource(url, params, actions, options) {

	    var self = this, resource = {};

	    actions = _.extend({},
	        Resource.actions,
	        actions
	    );

	    _.each(actions, function (action, name) {

	        action = _.merge({url: url, params: params || {}}, options, action);

	        resource[name] = function () {
	            return (self.$http || _.http)(opts(action, arguments));
	        };
	    });

	    return resource;
	}

	function opts(action, args) {

	    var options = _.extend({}, action), params = {}, data, success, error;

	    switch (args.length) {

	        case 4:

	            error = args[3];
	            success = args[2];

	        case 3:
	        case 2:

	            if (_.isFunction(args[1])) {

	                if (_.isFunction(args[0])) {

	                    success = args[0];
	                    error = args[1];

	                    break;
	                }

	                success = args[1];
	                error = args[2];

	            } else {

	                params = args[0];
	                data = args[1];
	                success = args[2];

	                break;
	            }

	        case 1:

	            if (_.isFunction(args[0])) {
	                success = args[0];
	            } else if (/^(POST|PUT|PATCH)$/i.test(options.method)) {
	                data = args[0];
	            } else {
	                params = args[0];
	            }

	            break;

	        case 0:

	            break;

	        default:

	            throw 'Expected up to 4 arguments [params, data, success, error], got ' + args.length + ' arguments';
	    }

	    options.data = data;
	    options.params = _.extend({}, options.params, params);

	    if (success) {
	        options.success = success;
	    }

	    if (error) {
	        options.error = error;
	    }

	    return options;
	}

	Resource.actions = {

	    get: {method: 'GET'},
	    save: {method: 'POST'},
	    query: {method: 'GET'},
	    update: {method: 'PUT'},
	    remove: {method: 'DELETE'},
	    delete: {method: 'DELETE'}

	};

	module.exports = _.resource = Resource;


/***/ },
/* 73 */
/***/ function(module, exports, __webpack_require__) {

	__webpack_require__(74);

	module.exports = {
		template: __webpack_require__(76),
		data: function () {
			return {
				selectedTab: false,
				slideLounge: false,
				slideEvent: false,
				state: {
					own: 1,
					lounge: 2,
					event: 3,
					finish: 4
				}
			}
		},
		props: ['battles', 'thumbs', 'isFilter'],
		methods: {
			eventFilter: function (battle) {
				if (this.isFilter) {
					if (this.$root.isUla) {
						return battle.mstGradeId == 3 && battle.gradeType == 4;
					} else if (this.$root.isLive) {
						return (battle.rarityType == 21 && battle.gradeType == 4) || battle.rarityType == 31
					} else if (this.$root.isRaid) {
						return (battle.rarityType == 21 && battle.gradeType == 4) || (battle.rarityType == 22 && battle.gradeType == 4) || battle.rarityType == 31
					} else if (this.$root.isPvp) {
						return (battle.rarityType == 31)
					}
				}
				return true;
			},
			changeTab: function (num) {
				this.selectedTab = num;
				this.$root.soundPlay('SE_SELECT');
			},
			clickSlideLounge: function () {
				$('#slide-btn-l-cont-lounge').slideToggle(400);
				if (this.slideLounge === false) {
					this.slideLounge = true;
				} else {
					this.slideLounge = false;
				}
			},
			clickSlideEvent: function () {
				$('#slide-btn-l-cont-event').slideToggle(400);
				if (this.slideEvent === false) {
					this.slideEvent = true;
				} else {
					this.slideEvent = false;
				}
			}
		},
		created: function () {
			if (localStorage.getItem('imas.liveBattle.eventFilter') !== null) {
				this.isFilter = !!parseInt(localStorage.getItem('imas.liveBattle.eventFilter'), 10);
			}
		},
		watch: {
			isFilter: function (val) {
				localStorage.setItem('imas.liveBattle.eventFilter', val ? 1 : 0);
				if (!this.$root.isLoading) {
					this.$root.load();
				}
			},
			battles: function (val) {
				if (this.selectedTab !== false)return;
				if (this.$root.initialSelectTab !== "") {
					this.selectedTab = this.state[this.$root.initialSelectTab];
				} else if (val.stats.lounge > 0) {
					this.selectedTab = this.state['lounge'];
				} else if (val.openEvent === true) {
					this.selectedTab = this.state['event'];
				} else if (val.stats.own > 0) {
					this.selectedTab = this.state['own'];
				} else if (val.stats.finish > 0) {
					this.selectedTab = this.state['finish'];
				} else {
					this.selectedTab = this.state['own'];
				}
			}
		},
		components: {
			item: __webpack_require__(77)
		}
	};


